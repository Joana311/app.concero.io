.container {
  padding-bottom: var(--sp-md);
  flex-direction: row;
  gap: var(--sp-xs);
  flex-wrap: wrap;

  .tagText {
    color: var(--color-base-white);
  }
}

.selectItemContainer {
  width: 100%;
  justify-content: space-between;
  align-items: center;
  flex-direction: row;

  .infoContainer {
    gap: var(--sp-sm);
    flex-direction: row;
    align-items: center;
  }
}
