.container {
	width: 100%;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
}

.tokenInfoContainer {
	flex-direction: row;
	align-items: center;
	gap: var(--sp-md);
}

.tokenImageContainer {
	position: relative;
}

.chainLogo {
	position: absolute;
	bottom: -3px;
	right: -3px;
}

.tokenTitleContainer {
}

.tokenPriceContainer {
	align-items: flex-end;
}

.balanceSkeleton {
	width: 30px;
	height: 16px;
	border-radius: var(--st-br-md);
}
